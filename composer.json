{
    "name": "zjwshisb/process-manager",
    "type": "library",
    "description": "A simple php process manager",
    "keywords": ["processs manager", "commands", "pcntl", "process"],
    "version": "1.0.4",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Zjwshisb\\ProcessManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zjwshisb\\ProcessManager\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "zhoujianwei",
            "email": "361340272@qq.com"
        }
    ],
    "require": {
        "php" : "^8.1",
        "symfony/process": "^6.4",
        "monolog/monolog": "^3.7",
        "ext-pcntl": "*",
        "ext-posix": "*"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.12",
        "phpunit/phpunit": "^10",
        "laravel/pint": "^1.18"
    },
    "scripts": {
        "post-merge": "composer install",
        "phpstan": "phpstan analyse --memory-limit=-1",
        "check-style": "vendor/bin/pint --test",
        "fix-style": "vendor/bin/pint",
        "test": "phpunit --colors"
    }
}
